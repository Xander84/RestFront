tr(Day) + '&#34; ';&#13;&#10;  if Month = 1 then&#13;&#10;    MonthStr := 'января'                &#13;&#10;  else if Month = 2 then&#13;&#10;    MonthStr := 'февраля'&#13;&#10;  else if Month = 3 then&#13;&#10;    MonthStr := 'марта'&#13;&#10;  else if Month = 4 then&#13;&#10;    MonthStr := 'апреля'&#13;&#10;  else if Month = 5 then&#13;&#10;    MonthStr := 'мая'&#13;&#10;  else if Month = 6 then&#13;&#10;    MonthStr := 'июня'&#13;&#10;  else if Month = 7 then&#13;&#10;    MonthStr := 'июля'&#13;&#10;  else if Month = 8 then&#13;&#10;    MonthStr := 'августа'&#13;&#10;  else if Month = 9 then&#13;&#10;    MonthStr := 'сентября'&#13;&#10;  else if Month = 10 then&#13;&#10;    MonthStr := 'октября'&#13;&#10;  else if Month = 11 then&#13;&#10;    MonthStr := 'ноября'&#13;&#10;  else if Month = 12 then&#13;&#10;    MonthStr := 'дакабря';&#13;&#10;&#13;&#10;  Str := Str + MonthStr + ' ' + IntToStr(Y) + 'г.';&#13;&#10;  Memo38.Text := Str;          &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." ShowProgress="False" OnBeforeReportPrint="OnBeforeReportPrint" PropData="">
